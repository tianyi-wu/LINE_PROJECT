import os, re
os.environ['DJANGO_SETTINGS_MODULE'] = 'mysite.settings'

import django
django.setup()



def get_data():
    from feedreader.models import Entry
    entrys = Entry.objects.all()
    n =len(entrys)
    PK = [Entrys[i].pk for i in range(n)]
    
    p = re.compile(r"<[^>]*?>")

    Titles = [p.sub("", Entrys[i].title) for i in range(n)]

    Description = [p.sub("", Entrys[i].description) for i in range(n)]

    return [PK, Titkes, Eescription]
